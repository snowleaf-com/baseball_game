{
  "name": "baseball-game-frontend",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.10.0",
  "dependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.68",
    "@types/react": "18.2.42",
    "@types/react-dom": "18.2.17",
    "axios": "1.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "7.0.0",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.32",
    "react-scripts": "5.0.1",
    "tailwindcss": "3.3.6",
    "typescript": "4.9.5",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "prettier": "3.2.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "type-check": "tsc --noEmit",
    "prepare": "test -d ../.git && cd .. && ./frontend/node_modules/.bin/husky install .husky || echo 'Skipping husky install (no git repo)'"
  },
  "lint-staged": {
    "frontend/src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "frontend/src/**/*.{json,css}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8080"
}
